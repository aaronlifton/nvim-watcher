func Test_ExecuteCommand(t *testing.T) {
	cmd := UpdatePluginsCmd
	b := bytes.NewBufferString("")
	cmd.SetArgs([]string{})
	cmd.SetOut(b)
	cmd.Execute()
	out, err := io.ReadAll(b)
	if err != nil {
		t.Fatal(err)
	}
	if string(out) != "hi" {
		t.Fatalf("expected \"%s\" got \"%s\"", "hi", string(out))
	}
}
